{
  "name": "stinkster-ui",
  "version": "1.0.0",
  "description": "Raspberry Pi-based SDR and WiFi intelligence platform with TAK integration",
  "keywords": [
    "sdr",
    "hackrf",
    "wifi",
    "kismet",
    "tak",
    "gps",
    "raspberry-pi",
    "iot",
    "security",
    "monitoring",
    "radio",
    "spectrum-analyzer"
  ],
  "homepage": "https://github.com/yourusername/stinkster#readme",
  "bugs": {
    "url": "https://github.com/yourusername/stinkster/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yourusername/stinkster.git"
  },
  "license": "MIT",
  "author": "Your Name <your.email@example.com>",
  "main": "index.js",
  "scripts": {
    "build:ts": "tsc",
    "watch:ts": "tsc --watch",
    "dev": "concurrently \"npm:watch:*\"",
    "dev:hackrf": "vite --config svelte-ui/vite.config.hackrf.js",
    "dev:wigle": "vite --config svelte-ui/vite.config.wigle.js",
    "dev:kismet": "vite --config svelte-ui/vite.config.kismet.js",
    "build": "npm run clean && npm run build:ts && npm run build:css",
    "build:all": "npm run build:hackrf && npm run build:wigle && npm run build:kismet",
    "build:hackrf": "vite build --config vite.config.hackrf.js",
    "build:wigle": "vite build --config vite.config.wigle.js",
    "build:kismet": "vite build --config vite.config.kismet.js",
    "build:prod": "NODE_ENV=production npm run build:prod:hackrf && NODE_ENV=production npm run build:prod:wigle && NODE_ENV=production npm run build:prod:kismet",
    "build:prod:hackrf": "vite build --config vite.config.optimized.js --mode production -- hackrf",
    "build:prod:wigle": "vite build --config vite.config.optimized.js --mode production -- wigle",
    "build:prod:kismet": "vite build --config vite.config.optimized.js --mode production -- kismet",
    "analyze:hackrf": "ANALYZE=true npm run build:prod:hackrf",
    "analyze:wigle": "ANALYZE=true npm run build:prod:wigle",
    "analyze:kismet": "ANALYZE=true npm run build:prod:kismet",
    "preview": "vite preview",
    "build:css": "tailwindcss -i ./src/styles.css -o ./assets/css/styles.css --minify",
    "watch": "concurrently \"npm:watch:*\"",
    "watch:css": "tailwindcss -i ./src/styles.css -o ./assets/css/styles.css --watch",
    "clean": "rimraf dist coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml,css,scss}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,yml,yaml,css,scss}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "test:coverage": "jest --coverage",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "commit": "cz",
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "build:critical": "node scripts/extract-critical.js",
    "analyze": "node scripts/analyze-bundle.js",
    "serve": "npx http-server . -p 8080"
  },
  "dependencies": {
    "@sveltejs/vite-plugin-svelte": "^5.1.0",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "svelte": "^5.34.7",
    "vite": "^6.3.5",
    "@vitejs/plugin-legacy": "^5.2.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-pwa": "^0.17.4",
    "workbox-window": "^7.0.0",
    "terser": "^5.26.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/socket.io-client": "^1.4.36",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-security": "^2.1.0",
    "eslint-plugin-unicorn": "^50.0.1",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.0",
    "nodemon": "^3.0.3",
    "postcss": "^8.5.6",
    "prettier": "^3.2.4",
    "rimraf": "^5.0.5",
    "rollup-plugin-visualizer": "^5.12.0",
    "standard-version": "^9.5.0",
    "tailwindcss": "^4.1.10",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "files": [
    "assets/",
    "src/",
    "index.html",
    "*.html",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "SECURITY.md"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
