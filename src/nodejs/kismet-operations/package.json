{
  "name": "stinkster-kismet-operations",
  "version": "2.0.0",
  "description": "Node.js Kismet Operations Center - WiFi scanning and monitoring dashboard",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc && npm run build:webpack",
    "build:watch": "tsc --watch",
    "build:webpack": "webpack --mode production",
    "build:webpack:dev": "webpack --mode development",
    "build:webpack:analyze": "ANALYZE=true webpack --mode production",
    "start": "node dist/server.js",
    "start:prod": "NODE_ENV=production node dist/server.js",
    "dev": "ts-node-dev --respawn --transpile-only server.ts",
    "dev:js": "nodemon server.js",
    "dev:webpack": "webpack --mode development --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint . --ext .ts,.js",
    "lint:fix": "eslint . --ext .ts,.js --fix",
    "typecheck": "tsc --noEmit",
    "clean": "rimraf dist",
    "prebuild": "npm run clean"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^7.2.1",
    "glob": "^11.0.3",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.5",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "node-gpsd": "^0.3.4",
    "ps-list": "^8.1.1",
    "socket.io": "^4.7.2",
    "socket.io-client": "^4.8.1",
    "tree-kill": "^1.2.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.23.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/lodash": "^4.14.202",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.4",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^10.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.53.0",
    "html-webpack-plugin": "^5.5.3",
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "rimraf": "^5.0.5",
    "style-loader": "^3.3.3",
    "supertest": "^6.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3",
    "webpack": "^5.89.0",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-cli": "^5.1.4"
  },
  "keywords": [
    "kismet",
    "wifi-scanning",
    "operations-center",
    "wireless-monitoring",
    "network-discovery",
    "dashboard",
    "websocket",
    "real-time"
  ],
  "author": "Stinkster Project Contributors",
  "license": "MIT"
}
